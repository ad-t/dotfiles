Xft.dpi: 200
polybar.dpi: 100
Xft.antialias: 1
Xcursor*theme: Capitaine
Xcursor*size: 128

*background: #231E18
*foreground: #CCCCCC

URxvt*termName: rxvt-unicode

URxvt*font: xft:Inconsolata for Powerline:size=14:antialias=true, xft:DejaVu Sans:size=7:antialias=true, xft:FontAwesome:size=7:antialias=true, xft:Icons:size=9:antialias=true
URxvt*letterSpace: 0
URxvt*saveLines: 1000000
URxvt*scrollBar: false
URxvt.perl-ext-common: default,matcher,font-size,selection-to-clipboard,pasta
URxvt.keysym.Control-Shift-V: perl:pasta:paste
URxvt.keysym.Control-0x2b: perl:font-size:increase
URxvt.keysym.Control-0x2d: perl:font-size:decrease
! pixels
URxvt.internalBorder: 32
URxvt.dynamicColors: on

! Rofi config
rofi.font: mono 16

