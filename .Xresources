Xft.dpi: 200
Xft.antialias: 1
Xcursor.size: 256

*background: #231E18
*foreground: #CCCCCC

URxvt*font: xft:Anonymice Powerline:size=12:antialias=true, xft:DejaVu Sans:size=12:antialias=true
URxvt*letterSpace: 0
URxvt*saveLines: 1000000
URxvt*scrollBar: false
URxvt.perl-ext-common: default,matcher,font-size
URxvt.keysym.Control-0x2b: perl:font-size:increase
URxvt.keysym.Control-0x2d: perl:font-size:decrease
! pixels
URxvt.internalBorder: 32

! *color0:  #191726
*color0:  #231E18
*color1:  #FF5C57
*color2:  #FFB536
*color3:  #F3F99D
*color4:  #5AF78E
*color5:  #9AEDFE
*color6:  #57C7FF
*color7:  #FFFFFF
! *color8:  #191726
*color8:  #231E18
*color9:  #FF5C57
*color10: #FFB536
*color11: #F3F99D
*color12: #5AF78E
*color13: #9AEDFE
*color14: #57C7FF
*color15: #FFFFFF
